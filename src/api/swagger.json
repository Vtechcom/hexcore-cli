        "components": {
            "securitySchemes": {
                "bearer": {
                    "scheme": "bearer",
                    "bearerFormat": "JWT",
                    "type": "http"
                }
            },
            "schemas": {
                "AdminLoginDto": {
                    "type": "object",
                    "properties": {
                        "username": {
                            "type": "string",
                            "description": "Admin username",
                            "example": "admin"
                        },
                        "password": {
                            "type": "string",
                            "description": "Admin password",
                            "example": "strongpassword123"
                        }
                    },
                    "required": [
                        "username",
                        "password"
                    ]
                },
                "CreateAccountDto": {
                    "type": "object",
                    "properties": {
                        "mnemonic": {
                            "type": "string",
                            "description": "Mnemonic for the new account",
                            "example": "day deal mimic mandate model clever network midnight much pudding strategy warrior"
                        }
                    },
                    "required": [
                        "mnemonic"
                    ]
                },
                "CreateHydraNodeDto": {
                    "type": "object",
                    "properties": {
                        "fromAccountId": {
                            "type": "number",
                            "description": "Account ID from which the Hydra node will be created",
                            "example": 1
                        },
                        "description": {
                            "type": "string",
                            "description": "Description for the Hydra node",
                            "example": "My first Hydra node"
                        }
                    },
                    "required": [
                        "fromAccountId",
                        "description"
                    ]
                },
                "CreatePartyDto": {
                    "type": "object",
                    "properties": {
                        "nodes": {
                            "type": "number",
                            "description": "Number of nodes for the party",
                            "example": 1
                        },
                        "description": {
                            "type": "string",
                            "description": "Description of the party",
                            "example": "Test Party"
                        },
                        "cardanoAccountIds": {
                            "description": "List of Cardano Account IDs",
                            "example": [
                                1
                            ],
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "required": [
                        "nodes",
                        "description",
                        "cardanoAccountIds"
                    ]
                },
                "ReqActivePartyDto": {
                    "type": "object",
                    "properties": {}
                },
                "ReqClearPartyDataDto": {
                    "type": "object",
                    "properties": {
                        "ids": {
                            "example": [
                                1,
                                2,
                                3
                            ],
                            "description": "Array of Hydra Party ID",
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "required": [
                        "ids"
                    ]
                },
                "BigIntResponseDto": {
                    "type": "object",
                    "properties": {
                        "data": {
                            "type": "object",
                            "description": "The actual response data"
                        },
                        "bigIntFields": {
                            "description": "List of field paths that contained BigInt values",
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "required": [
                        "data"
                    ]
                },
                "ProtocolParametersDto": {
                    "type": "object",
                    "properties": {
                        "maxBlockBodySize": {
                            "type": "string",
                            "description": "Maximum block body size in bytes"
                        },
                        "maxTransactionSize": {
                            "type": "string",
                            "description": "Maximum transaction size in bytes"
                        },
                        "minFeeCoefficient": {
                            "type": "string",
                            "description": "Minimum fee coefficient A"
                        },
                        "minFeeConstant": {
                            "type": "string",
                            "description": "Minimum fee constant B"
                        },
                        "minUtxoValue": {
                            "type": "string",
                            "description": "Minimum UTxO value"
                        },
                        "stakePoolDeposit": {
                            "type": "string",
                            "description": "Pool deposit"
                        },
                        "stakeKeyDeposit": {
                            "type": "string",
                            "description": "Key deposit"
                        }
                    },
                    "required": [
                        "maxBlockBodySize",
                        "maxTransactionSize",
                        "minFeeCoefficient",
                        "minFeeConstant",
                        "minUtxoValue",
                        "stakePoolDeposit",
                        "stakeKeyDeposit"
                    ]
                },
                "HealthResponseDto": {
                    "type": "object",
                    "properties": {
                        "healthy": {
                            "type": "boolean",
                            "description": "Whether the service is healthy"
                        },
                        "timestamp": {
                            "type": "string",
                            "description": "Timestamp of the health check"
                        }
                    },
                    "required": [
                        "healthy",
                        "timestamp"
                    ]
                }
            }
        }
    },
    "customOptions": {}
